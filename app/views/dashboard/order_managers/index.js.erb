$('.history-order-done').html('<%= j render "order_product", order_products: @order_products_done, type: :done %>')
$('.history-order-reject').html('<%= j render "order_product", order_products: @order_products_reject, type: :rejected %>')
<% if @order_products_done.size == 0 && @order_products_reject.size == 0 %>
  sweetAlert(I18n.t("load_order"), '', 'info');
<% end %>
